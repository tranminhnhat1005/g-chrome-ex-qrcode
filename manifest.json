{
  "manifest_version": 3,
  "name": "QR Code Generator Pro",
  "version": "2.3.0",
  "description": "Professional QR code generator with PNG/SVG download, dark blue theme, customizable sizes up to 1024px - Chrome Web Store ready",
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "src/assets/icons/icon16.png",
      "48": "src/assets/icons/icon48.png",
      "128": "src/assets/icons/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [],
  "icons": {
    "16": "src/assets/icons/icon16.png",
    "48": "src/assets/icons/icon48.png",
    "128": "src/assets/icons/icon128.png"
  }
}
